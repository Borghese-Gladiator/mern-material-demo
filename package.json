{
	"name": "mern-material-demo",
	"version": "0.0.1",
	"description": "MERN stack auth demo with Material UI.",
	"main": "index.js",
	"scripts": {
		"client-build": "npm run build --prefix client",
		"dev": "npm run client-build && babel-watch server/index.js",
		"test": "echo \"Error: no test specified\" && exit 1",
		"clean": "rm -rf build && mkdir build",
		"build-babel": "babel -d ./build ./server -s",
		"build": "npm run clean && npm run build-babel",
		"start": "node ./build/index.js",
		"heroku-postbuild": "npm run build && npm install --prefix client && npm run client-build"
	},
	"keywords": [],
	"author": "Aman Mittal <amandeepmittal@live.com> (www.amanhimself.me)",
	"license": "MIT",
	"dependencies": {
		"cookie-parser": "^1.4.3",
		"dotenv": "^8.2.0",
		"express": "^4.16.4",
		"express-jwt": "^5.3.1",
		"jsonwebtoken": "^8.3.0",
		"mongoose": "^5.3.8"
	},
	"devDependencies": {
		"@babel/cli": "^7.12.10",
		"@babel/core": "^7.12.10",
		"@babel/preset-env": "^7.12.11",
		"babel-watch": "^7.3.0"
	},
	"engines": {
		"node": "12.x"
	}
}
